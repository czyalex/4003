@prefix :    <http://example.org/ae-kg#> .
@prefix rdf:  <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd:  <http://www.w3.org/2001/XMLSchema#> .
@prefix dct:  <http://purl.org/dc/terms/> .
@prefix prov: <http://www.w3.org/ns/prov#> .

:Drug        a rdfs:Class .
:Protein     a rdfs:Class .
:Gene        a rdfs:Class .
:Phenotype   a rdfs:Class .
:MedDRATerm  a rdfs:Class .
:Evidence    a rdfs:Class .

:actsOn      a rdf:Property ; rdfs:domain :Drug ;     rdfs:range :Protein .
:encodedBy   a rdf:Property ; rdfs:domain :Protein ;  rdfs:range :Gene .
:hasPhenotype a rdf:Property ; rdfs:domain :Gene ;    rdfs:range :Phenotype .

:hpoMappedToMedDRA a rdf:Property ; rdfs:domain :Phenotype ; rdfs:range :MedDRATerm .
:mayCause    a rdf:Property ; rdfs:domain :Drug ;     rdfs:range :Phenotype .

:weight      a rdf:Property ; rdfs:domain :Evidence ; rdfs:range xsd:float .
:about       a rdf:Property ; rdfs:domain :Evidence ; rdfs:range rdf:Resource .
:source      a rdf:Property ; rdfs:domain :Evidence ; rdfs:range xsd:string .

# Labels (optional)
:actsOn rdfs:label "acts on" .
:encodedBy rdfs:label "encoded by" .
:hasPhenotype rdfs:label "has phenotype" .
:hpoMappedToMedDRA rdfs:label "HPO mapped to MedDRA" .
:mayCause rdfs:label "may cause" .
